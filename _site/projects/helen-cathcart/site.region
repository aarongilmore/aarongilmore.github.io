<!doctype html>

<html>

  <head>
    
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <meta name="viewport" content="width=device-width,initial-scale=1,maximum-scale=1">

    <!--INCLUDE SQS SCRIPTS, META TAGS AND USER CONTENT FROM THE CODE INJECTION TAB-->
	<link href="//cloud.webtype.com/css/9d110faa-4f0d-4cf6-8538-4f1d160dde4f.css" rel="stylesheet" type="text/css" />
    {squarespace-headers}
	


  
  </head>

  <body id="{squarespace.page-id}" class="{squarespace.page-classes}">
    
    <!--HEADER-->

    <header id="header">

      <!--SITE TITLE OR LOGO-->

      {.section website}

        <h1 data-content-field="site-title">
          <a href="/">
            {.section logoImageUrl}
             <img src="{logoImageUrl}?format=original" alt="{siteTitle}" />
            {.or}
              {siteTitle}
            {.end}
          </a>
        </h1>
        
        <!--SITE TAGLINE-->

        {.section siteTagLine}<p>{@}</p>{.end}

      {.end}

      <!--MAIN NAVIGATION-->

      <squarespace:navigation navigationId="mainNav" template="navigation" />

    </header>

    <!--CONTENT INJECTION POINT-->

    <section id="content">    
     {squarespace.main-content}
    </section>

    <!--FOOTER WITH OPEN BLOCK FIELD-->

    <footer id="footer">
    <!--  <squarespace:block-field id="footerBlocks" columns="12" />-->
	<small>Copyright &copy; 2013â€”2014 Helen Cathcart</small>
    </footer>

    <!--INJECTION POINT FOR TRACKING SCRIPTS AND USER CONTENT FROM THE CODE INJECTION TAB-->

    {squarespace-footers}
	<script type="text/javascript" charset="utf-8">
	$( document ).ready(function() {
		if ($("body.collection-type-horizontalgallery").length == 1) {
		 $(".hint-text").hide();
		};
		});
	</script>
	
	<squarespace:script src="helen.js" combo="true" />
	

	<squarespace:script src="MyFontsWebfontsKit.js" combo="true" />
	

    
  </body>

</html> 